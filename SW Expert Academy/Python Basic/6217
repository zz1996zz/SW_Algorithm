class Student:
    name='홍길동'
    
class GraduateStudent(Student):
    def __init__(self,name,major):
        self.name=name
        self.major=major
        
    
    def show(self):
        print("이름: {0}" .format(Student.name))
        print("이름: {0}, 전공: {1}" .format(self.name,self.major))
        
stu=GraduateStudent('이순신','컴퓨터')
stu.show()
